{
  "name": "todo-bot",
  "version": "1.10.2",
  "description": "Todo bot for mick, maybe going to make this somewhat advanced",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon TODOBOT/index.js",
    "start": "node TODOBOT/index.js",
    "dc:up": "docker-compose up -d",
    "dc:down": "docker-compose down",
    "update": "ncu -u",
    "upgrade": "ncu -u && npm install",
    "commit": "cz",
    "release": "standard-version",
    "release:pre": "npm run release -- --prerelease",
    "release:alpha": "npm run release -- --prerelease alpha",
    "release:beta": "npm run release -- --prerelease beta",
    "register": "node TODOBOT/modules/util/register.js"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "author": "MeerBiene",
  "license": "WTFPL",
  "dependencies": {
    "@aero/centra": "^1.0.6",
    "@elastic/ecs-pino-format": "^1.1.1",
    "agenda": "^4.1.2",
    "blapi": "^2.1.0",
    "bufferutil": "^4.0.3",
    "date-fns": "^2.20.1",
    "discord-paginationembed": "^2.1.0",
    "discord.js-light": "^3.5.9",
    "dotenv": "^8.2.0",
    "elastic-apm-node": "^3.13.0",
    "enmap": "^5.8.4",
    "erlpack": "github:discord/erlpack",
    "express": "^4.17.1",
    "file-type": "^16.3.0",
    "fs": "0.0.1-security",
    "mongoose": "^5.12.3",
    "pino": "^6.11.2",
    "prompts": "^2.4.1",
    "redis": "^3.1.0",
    "slash-commands": "^1.4.0",
    "standard-version": "^9.2.0",
    "utf-8-validate": "^5.0.4",
    "uuid": "^8.3.2",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "commitizen": "^4.2.3",
    "standard-version": "^9.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shitcorp/TODOBOT"
  },
  "bugs": {
    "url": "https://github.com/shitcorp/TODOBOT/issues"
  }
}
